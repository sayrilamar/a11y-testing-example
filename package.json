  },
  "dependencies": {
    "gatsby": "^2.20.12",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "axe-core": "^3.5.3",
    "cypress": "^4.3.0",
    "cypress-axe": "^0.8.1",
    "gatsby-cypress": "^0.3.1",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
